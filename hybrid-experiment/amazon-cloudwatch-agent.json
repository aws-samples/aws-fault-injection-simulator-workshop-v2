{
	"agent": {
		"metrics_collection_interval": 60,
		"run_as_user": "cwagent"
	},
	"metrics": {
		"metrics_collected": {
			"collectd": {
				"metrics_aggregation_interval": 60
			},
			"cpu": {
				"measurement": [
					"cpu_usage_idle",
					"cpu_usage_iowait",
					"cpu_usage_active",
					"cpu_usage_guest",
					"cpu_usage_guest_nice",
					"cpu_usage_irq",
					"cpu_usage_nice",
					"cpu_usage_softirq",
					"cpu_usage_steal",
					"cpu_usage_system",
					"cpu_usage_user",
					"cpu_time_guest",
					"cpu_time_guest_nice",
					"cpu_time_idle",
					"cpu_time_iowait",
					"cpu_time_irq",
					"cpu_time_nice",
					"cpu_time_softirq",
					"cpu_time_steal",
					"cpu_time_system",
					"cpu_time_user"
				],
				"metrics_collection_interval": 60,
				"resources": [
					"*"
				],
				"totalcpu": true
			},
			"disk": {
				"measurement": [
					"disk_used_percent",
					"disk_free",
					"disk_total"
				],
				"metrics_collection_interval": 60,
				"resources": [
					"*"
				]
			},
			"diskio": {
				"measurement": [
					"io_time",
					"write_bytes",
					"read_bytes",
					"writes",
					"reads"
				],
				"metrics_collection_interval": 60,
				"resources": [
					"*"
				]
			},
			"mem": {
				"measurement": [
					"mem_used_percent",
					"mem_total",
					"mem_free",
					"mem_used"
				],
				"metrics_collection_interval": 60
			},
			"net": {
				"measurement": [
					"bytes_sent",
					"bytes_recv",
					"packets_sent",
					"packets_recv"
				],
				"metrics_collection_interval": 60,
				"resources": [
					"*"
				]
			},
			"swap": {
				"measurement": [
					"swap_used_percent"
				],
				"metrics_collection_interval": 60
			}
		}
	}
}
